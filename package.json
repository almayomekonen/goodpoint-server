{
    "name": "server",
    "version": "0.0.1",
    "description": "gp server",
    "author": "hilma",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\"",
        "start": "nest start",
        "start:dev": "NODE_ENV=development nest start --watch",
        "start:dev:preserve": "NODE_ENV=development nest start --watch --preserveWatchOutput",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "postinstall": "npm run build",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "migration-run": "npm run build &&  npx typeorm-ts-node-esm migration:run -d src/migration-config.ts",
        "migration-revert": "npm run build && npx typeorm-ts-node-esm migration:revert -d src/migration-config.ts",
        "migration-generate": "npm run build && npx typeorm-ts-node-esm migration:generate -d src/migration-config.ts ",
        "test:firebase": "node test-firebase-auth.js"
    },
    "dependencies": {
        "@babel/preset-env": "^7.23.6",
        "@hilma/auth-nest": "=0.6.2-beta.1",
        "@nestjs/common": "^10.3.0",
        "@nestjs/config": "^3.0.0",
        "@nestjs/core": "^10.3.0",
        "@nestjs/jwt": "^10.2.0",
        "@nestjs/platform-express": "^10.1.0",
        "@nestjs/platform-socket.io": "^10.1.0",
        "@nestjs/schedule": "^4.0.1",
        "@nestjs/serve-static": "^4.0.0",
        "@nestjs/swagger": "^7.1.2",
        "@nestjs/typeorm": "^10.0.0",
        "@nestjs/websockets": "^10.1.0",
        "@react-email/components": "^0.0.14",
        "@react-email/render": "^0.0.12",
        "@react-pdf/renderer": "^3.3.5",
        "@sendgrid/mail": "^8.1.0",
        "@types/randomstring": "^1.1.11",
        "babel-jest": "^29.7.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "cookie-parser": "^1.4.6",
        "crypto-ts": "^1.0.2",
        "firebase-admin": "^13.4.0",
        "jszip": "^3.10.1",
        "multer": "^1.4.5-lts.1",
        "mysql2": "^3.14.2",
        "openai": "^3.3.0",
        "openid-client": "^5.6.4",
        "randomstring": "^1.3.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^5.0.5",
        "rxjs": "^7.8.1",
        "typeorm": "^0.3.17",
        "xlsx": "^0.18.5",
        "yup": "^1.3.3"
    },
    "devDependencies": {
        "@faker-js/faker": "^8.3.1",
        "@nestjs/cli": "^10.3.0",
        "@nestjs/schematics": "^10.1.1",
        "@nestjs/testing": "^10.3.0",
        "@types/cookie-parser": "^1.4.6",
        "@types/cron": "^2.0.1",
        "@types/express": "^4.17.17",
        "@types/jest": "29.5.12",
        "@types/multer": "^1.4.7",
        "@types/node": "^20.4.2",
        "@types/react": "18.2.0",
        "@types/react-dom": "18.2.0",
        "@types/react-pdf": "^6.2.0",
        "@types/socket.io": "^3.0.2",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^6.2.0",
        "@typescript-eslint/parser": "^6.2.0",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-check-file": "^2.6.2",
        "eslint-plugin-prettier": "^5.1.2",
        "eslint-plugin-react": "^7.33.1",
        "jest": "^29.7.0",
        "prettier": "^3.0.0",
        "source-map-support": "^0.5.21",
        "supertest": "^6.3.4",
        "ts-jest": "^29.1.1",
        "ts-loader": "^9.4.4",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "4.2.0",
        "typescript": "^5.1.6"
    },
    "engines": {
        "node": ">=20.0.0",
        "npm": ">=10.0.0"
    }
}
